{
  "name": "calenderjs",
  "version": "0.1.0",
  "private": true,
  "description": "基于日历的预约管理系统，支持 Appointment DSL、多租户架构和第三方集成",
  "scripts": {
    "dev": "nx run-many --target=dev --parallel",
    "build": "nx run-many --target=build --parallel",
    "test": "nx run-many --target=test --parallel",
    "lint": "nx run-many --target=lint --parallel",
    "clean": "nx reset && nx run-many --target=clean --parallel",
    "graph": "nx graph"
  },
  "devDependencies": {
    "@nx/devkit": "^17.0.0",
    "@nx/js": "^17.0.0",
    "@nx/node": "^17.0.0",
    "@nx/next": "^17.0.0",
    "@nx/vite": "^17.0.0",
    "@nx/workspace": "^17.0.0",
    "nx": "^17.0.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
